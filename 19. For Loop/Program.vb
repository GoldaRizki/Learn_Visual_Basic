
Module forloop

    Sub main()

        ' Cara nggawe for loop ng kene

        For i As Integer = 1 To 5
            Console.WriteLine("Loop ke-{0}", i)
        Next

        ' Kita bisa specify nilai stepnya

        For j As Integer = 1 To 4 Step 3

            Console.WriteLine("Looping ke-{0}", j)

        Next


    End Sub


End Module